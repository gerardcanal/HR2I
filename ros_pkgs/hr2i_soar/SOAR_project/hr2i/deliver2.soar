sp {hr2i*propose*deliver
    (state <s> ^name hr2i
               ^location <l> 
               ^robot <r>   
               ^person <p>  
               ^object <o>         
               ^desired <d>)
    (<l> ^id <rloc>)
    (<r> ^locId <rloc>
         ^obj1Id <oid>)
    (<p> ^id <pid>
         ^locId <rloc>
         ^obj1Id -1
         ^found yes)
    (<o> ^id <oid>
         ^grasped yes)
    (<d> ^person <dp>
         ^object <do>)
    (<dp> ^id <pid>)
    (<do> ^id <oid>)
 -->
   (<s> ^operator <op> + >)
    (<op> ^name deliver
          ^toPers <p>)
}

sp {hr2i*propose*navigate-to*deliver
    (state <s> ^name hr2i
               ^location <l>
               ^robot <r>
               ^person <p>
               ^object <o>
               ^desired <d>)
    (<l> ^id <ploc>)
    (<r> ^locId {<> <ploc> <rloc>}
         ^obj1Id <oid>)
    (<p> ^id <pid>
         ^locId <ploc>
         ^obj1Id -1
         ^found no)
    (<o> ^id <oid>
         ^grasped yes)
    (<d> ^person <dp>
         ^object <do>)
    (<dp> ^id <pid>)
    (<do> ^id <oid>)
 -->
   (<s> ^operator <op> + >)
    (<op> ^name navigate
          ^toLoc <l>)
}


sp {hr2i*apply*deliver
   (state <s> ^operator <op> +
              ^io.output-link <out>)
   (<op> ^name deliver
         ^toPers <p>)
   (<p> ^id <pid>)
-->
   (<out> ^deliver.pers <pid>)
}

sp {hr2i*apply*deliver*remove-deliver
   (state <s> ^io.output-link <out>
              ^operator <op>
              ^person <p>
              ^object <o>
              ^robot <r>)
   (<out> ^deliver <st>)
   (<st> ^status complete
         ^pers <pid>)
   (<p> ^id <pid>
        ^obj1Id <poid>)
   (<o> ^id <oid>
        ^delivered <val>)
   (<r> ^id 1
        ^obj1Id <oid>
        ^locId <rloc>)
   
-->
   (<out> ^deliver <st> -)
   (<o> ^delivered yes
                   <val> -
        ^locId <rloc>)
   (<p> ^obj1Id <oid>
                <poid> -)
   
#   (<p> ^obj1Id <oid>
#                <poid> -)
   (<r> ^obj1Id -1
                <oid> -)
}