sp {hr2i*propose*deliverToLoc
   (state <s> ^name hr2i
              ^location <l>
              ^robot <r>
              ^object <o>
              ^desired <d>)
    (<l> ^id <rloc>)
    (<r> ^obj1Id <oid>
         ^locId <rloc>)
    (<o> ^id <oid>
         ^grasped yes
         ^delivered no)
    (<d> ^object <do>
        -^person <po>) #potser sa de borrar
    (<do> ^id <oid>
          ^locId <rloc>
          ^delivered yes)
 -->
    (<s> ^operator <op> + >)
    (<op> ^name deliver
          ^toLoc <l>)
}

