sp {hr2i*propose*navigate*deliverToLoc
   (state <s> ^name hr2i
              ^location <l>
              ^object <o>                       
              ^desired <d>
              ^robot <r>)
    (<l> ^id <iloc>)
    (<o> ^id <oid>
         ^grasped yes)
    (<d> ^object <do>)
    (<do> ^id <oid>
          ^locId <iloc>)
    (<r> ^locId {<> <iloc> <rloc>}
         ^obj1Id <oid>)
 -->
   (<s> ^operator <op> + >)
    (<op> ^name navigate
          ^toLoc <l>)
}

sp {hr2i*apply*deliverToLoc
   (state <s> ^operator <op> +
              ^io.output-link <out>)
   (<op> ^name deliver
         ^toLoc <l>)
   (<l> ^id <lid>)
-->
   (<out> ^deliver.loc <lid>)
}

sp {hr2i*apply*deliver*remove-deliverToLoc
   (state <s> ^operator <op> 
              ^io.output-link <out>
              #^location <l>  #...
              ^object <o>
              ^robot <r>)
   (<out> ^deliver <st>)
   (<st> ^status complete
         ^loc <rloc>)
   (<op> ^name deliver #comentat per mi
         ^toLoc <l>)
   (<l> ^id <lid>)
   (<o> ^id <oid>
        ^delivered <val1>
        ^locId <olocid> #comentat per mi
        ^grasped <val2>)#comentat per mi
   (<r> ^id 1
        ^obj1Id <oid>)
       # ^locId <rloc>)  #...
   
-->
   (<out> ^deliver <st> -)
   (<o> ^id <oid>   #comentat per mi
        ^delivered yes
                   <val1> -
        ^grasped no
                 <val2> -
        ^locId <rloc>
               <olocid> -)
   (<r> ^id 1   #comentat per mi
        ^obj1Id -1
                <oid> -)
}